<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
  <appSettings>
    <!-- Required -->
    <add key="ServiceStack.Plugins.IntroSpec.Contact.Name" value="Private Pile"/>
